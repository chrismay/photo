<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="currentCompetitionQuery" class="uk.co.blackpepper.photocomp.active.ActiveCompetitionRepository">
        <constructor-arg ref="mapStore"/>
    </bean>

    <bean id="competitionResultsQuery" class="uk.co.blackpepper.photocomp.active.CompetitionResultsRepository">
        <constructor-arg ref="mapStore"/>
    </bean>


</beans>
